<Movsim>
    <VehiclePrototypes>
        <VehiclePrototypeConfiguration label="ACC1" length="6" maximum_deceleration="9">
            <AccelerationModelType>
                <ModelParameterACC v0="35" T="1.2" s0="3" s1="2" delta="4" a="1.2" b="2.0" coolness="1" />
            </AccelerationModelType>
            <LaneChangeModelType european_rules="true" crit_speed_eur="20">
                <ModelParameterMOBIL safe_deceleration="5.0" minimum_gap="2.0" threshold_acceleration="0.1" right_bias_acceleration="0.05" politeness="0.1" />
            </LaneChangeModelType>
        </VehiclePrototypeConfiguration>
        <!-- trucks taking the exits -->
        <VehiclePrototypeConfiguration label="ACC2" length="16" maximum_deceleration="9">
            <AccelerationModelType>
                <ModelParameterACC v0="25" T="1.5" s0="4" s1="4" delta="4" a="0.8" b="2.0" coolness="1" />
            </AccelerationModelType>
            <LaneChangeModelType european_rules="true" crit_speed_eur="20">
                <ModelParameterMOBIL safe_deceleration="4.0" minimum_gap="2.0" threshold_acceleration="0.2" right_bias_acceleration="0.3" politeness="0.1" />
            </LaneChangeModelType>
        </VehiclePrototypeConfiguration>
        <VehiclePrototypeConfiguration label="ACC3" length="16" maximum_deceleration="9">
            <AccelerationModelType>
                <ModelParameterACC v0="20" T="1.5" s0="4" s1="4" delta="4" a="0.8" b="2.0" coolness="1" />
            </AccelerationModelType>
            <LaneChangeModelType european_rules="true" crit_speed_eur="20">
                <ModelParameterMOBIL safe_deceleration="4.0" minimum_gap="2.0" threshold_acceleration="0.2" right_bias_acceleration="0.3" politeness="0.1" />
            </LaneChangeModelType>
        </VehiclePrototypeConfiguration>
    </VehiclePrototypes>
    <Scenario network_filename="cleaf.xodr">
        <Simulation timestep="0.2" seed="42" crash_exit="false">
            <TrafficComposition>
                <VehicleType label="ACC1" fraction="0.8" relative_v0_randomization="0.2" />
                <VehicleType label="ACC2" fraction="0.2" relative_v0_randomization="0.2" />
            </TrafficComposition>
            <Road id="1">
                <TrafficComposition>
                    <VehicleType label="ACC1" fraction="0.9" relative_v0_randomization="0.2" />
                    <VehicleType label="ACC2" fraction="0.1" relative_v0_randomization="0.2" route_label="north" />
                    <VehicleType label="ACC3" fraction="0.1" relative_v0_randomization="0.2" route_label="north2" />
                </TrafficComposition>
                <TrafficSource logging="false">
                    <Inflow t="0" q_per_hour="1100" v="24" />
                </TrafficSource>
            </Road>
            <Road id="10">
                <TrafficComposition>
                    <VehicleType label="ACC1" fraction="0.9" relative_v0_randomization="0.2" />
                    <VehicleType label="ACC2" fraction="0.1" relative_v0_randomization="0.2" route_label="south" />
                    <VehicleType label="ACC3" fraction="0.1" relative_v0_randomization="0.2" route_label="south2" />
                </TrafficComposition>
                <TrafficSource>
                    <Inflow t="0" q_per_hour="1100" />
                </TrafficSource>
            </Road>
            <Road id="19">
                <TrafficComposition>
                    <VehicleType label="ACC1" fraction="0.9" relative_v0_randomization="0.2" />
                    <VehicleType label="ACC2" fraction="0.1" relative_v0_randomization="0.2" route_label="east" />
                    <VehicleType label="ACC3" fraction="0.1" relative_v0_randomization="0.2" route_label="east2" />
                </TrafficComposition>
                <TrafficSource>
                    <Inflow t="0" q_per_hour="1100" />
                </TrafficSource>
            </Road>
            <Road id="28">
                <TrafficComposition>
                    <VehicleType label="ACC1" fraction="0.9" relative_v0_randomization="0.2" />
                    <VehicleType label="ACC2" fraction="0.1" relative_v0_randomization="0.2" route_label="west" />
                    <VehicleType label="ACC3" fraction="0.1" relative_v0_randomization="0.2" route_label="west2" />
                </TrafficComposition>
                <TrafficSource>
                    <Inflow t="0" q_per_hour="1100" />
                </TrafficSource>
            </Road>
        </Simulation>
        <Routes>
            <!-- source south in direction north, take first exit in direction north -->
            <Route label="north">
                <Road id="1" />
                <Road id="2" />
                <Road id="37" />
            </Route>
            <!-- take second exit in direction north -->
            <Route label="north2">
                <Road id="1" />
                <Road id="2" />
                <Road id="3" />
                <Road id="4" />
                <Road id="5" />
                <Road id="6" />
                <Road id="41" />
            </Route>
            <!-- source north in driving direction south -->
            <Route label="south">
                <Road id="10" />
                <Road id="11" />
                <Road id="38" />
            </Route>
            <Route label="south2">
                <Road id="10" />
                <Road id="11" />
                <Road id="12" />
                <Road id="13" />
                <Road id="14" />
                <Road id="15" />
                <Road id="42" />
            </Route>
            <!-- source west direction east -->
            <Route label="west">
                <Road id="28" />
                <Road id="29" />
                <Road id="40" />
            </Route>
            <Route label="west2">
                <Road id="28" />
                <Road id="29" />
                <Road id="30" />
                <Road id="31" />
                <Road id="32" />
                <Road id="33" />
                <Road id="44" />
            </Route>
            <!-- source east direction west -->
            <Route label="east">
                <Road id="19" />
                <Road id="20" />
                <Road id="39" />
            </Route>
            <Route label="east2">
                <Road id="19" />
                <Road id="20" />
                <Road id="21" />
                <Road id="22" />
                <Road id="23" />
                <Road id="24" />
                <Road id="43" />
            </Route>
        </Routes>
    </Scenario>
</Movsim>

